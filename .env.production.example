# API Configuration
VITE_API_BASE_URL=https://api.production.com

# WebSocket Configuration  
VITE_WS_CHAT_URL=wss://api.production.com

# Google OAuth
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Logging
VITE_LOG_LEVEL=info

# Security
# Options: development | production | staging
# [NOTE] Currently ONLY a descriptive label. It does NOT affect app logic or build process.
# The app relies on `import.meta.env.DEV` (provided by Vite) for development checks.
VITE_ENVIRONMENT=production
VITE_ENABLE_DEBUG=false

# Microsft Analytics
VITE_CLARITY_ID=your_clarity_id
